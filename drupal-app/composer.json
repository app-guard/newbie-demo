{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
                "name":"enyo/dropzone",
                "version": "4.0.1",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/enyo/dropzone",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "heiseonline/shariff",
                "version": "master",
                "type": "drupal-library",
                "dist": {
                    "url": "https://github.com/heiseonline/shariff/archive/1.26.2.zip",
                    "type": "zip"
                },
                "require": {
                    "composer/installers": "~1.0"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/colorbutton",
                "version": "4.6.2",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/colorbutton/releases/colorbutton_4.6.2.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/panelbutton",
                "version": "4.7.3",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/panelbutton/releases/panelbutton_4.7.3.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "ckeditor/token",
                "version": "1.1.0",
                "type": "drupal-library",
                "dist": {
                    "url": "https://download.ckeditor.com/token/releases/token_1.1.zip",
                    "type": "zip"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name":"smonetti/btbutton",
                "version": "1.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/smonetti/btbutton",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "davidjbradshaw/iframe-resizer",
                "version": "3.5",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/davidjbradshaw/iframe-resizer",
                    "type": "git",
                    "reference": "origin/master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "bgrins/spectrum",
                "version": "1.6.0",
                "type": "drupal-library",
                "source": {
                    "url": "https://github.com/bgrins/spectrum",
                    "type": "git",
                    "reference": "tags/1.6.0"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^2.2",
        "pear/versioncontrol_git": "v0.5.0",
        "drupal/core": "8.4.5",
