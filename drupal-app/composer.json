        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8",
        "drupal/coder": "8.2.*",
        "phpmd/phpmd": "2.*",
        "sebastian/phpcpd": "2.*",
        "squizlabs/php_codesniffer": "2.*",
        "geerlingguy/drupal-vm": "^3.1",
        "drupal/devel": "~1.0",
        "jakub-onderka/php-parallel-lint": "^0.9.2"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "bin-dir": ".bin/",
        "secure-http": false,
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "extra": {
        "patches": {
            "drupal/default_content": {
                "#2698425 Do not reimport existing entities": "https://www.drupal.org/files/issues/do_not_reimport-2698425-64.patch",
                "#2640734 Allow manual imports": "https://www.drupal.org/files/issues/allow_manual_imports-2640734-67.patch"
            },
            "drupal/linkit": {
                "2903176 Views as a dependency": "https://www.drupal.org/files/issues/linkit_views_dependency-2903176-2.patch"
            },
            "drupal/paragraphs": {
                "#2868155 add new hooks to allow easier editing of paragraph forms": "https://www.drupal.org/files/issues/2868155-8.patch"
            },
            "drupal/menu_breadcrumb": {
                "Change label of the home link": "patches/menu_breadcrumb_change_home_label.patch"
            },
            "drupal/conditional_fields": {
                "Fix problems with notices": "patches/conditional_fields_fixed_php_notices.patch"
            },
            "drupal/eu_cookie_compliance": {
                "#2811003 Consent by clicking": "https://www.drupal.org/files/issues/eu_cookie_compliance_consent_by_clicking-2811003-4.patch",
                "Remove default settings": "patches/eu_cookie_compliance_fixes.patch"
            },
            "drupal/soundcloudfield": {
                "Remove title from fildset widget": "patches/soundcloudfield_remove_title_from_filedset.patch",
                "Changed curl for Guzzle HTTP client library": "patches/soundcloudfield_changed_curl_function.patch"
            },
            "drupal/shariff": {
                "#2881696 Missing module js file": "https://www.drupal.org/files/issues/two_twitter_popups-2881126-6.patch"
            },
            "drupal/core": {
                "#2881126 Media: Migration integration": "patches/2835825-21.patch",
                "add drowdrop callback to tabledrag": "patches/tabledrag_rowdrop_event.patch"
            },
            "drupal/migrate_plus": {
                "Fix issue #2792765 allow XML elements": "patches/migrate_plus_fix_2792765.patch"
            },
            "drupal/color_field": {
                "#2827198 Spectrum: Defining a palette": "https://www.drupal.org/files/issues/color_field_spectrum_palette_fix-2827198-18.patch"
            },
            "drupal/flag": {
                "#2825993 Fix Views support for anonymous flaggings": "https://www.drupal.org/files/issues/flag-views_support_anon_flaggins-2825993-58.patch"
            },
            "drupal/amptheme": {
                "#2753089 Check if simplexml_load_file returns FALSE in preprocess_block": "https://www.drupal.org/files/issues/amptheme-do_not_use_the_network_for_loading_logo_information-2753089-7.patch"
            },
            "drupal/styleswitcher": {
                "Improve behaviors for cookies": "patches/styleswitcher_cookie_domain.patch"
            },
            "drupal/encrypt": {
                "Added coninical route": "patches/ecrypt_added_canonical_route.patch"
            }
        },
        "installer-paths": {
            "docroot/core": ["type:drupal-core"],
            "docroot/modules/contrib/{$name}": ["type:drupal-module"],
            "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
            "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"],
             "docroot/libraries/{$name}": [
                "type:drupal-library",
                "enyo/dropzone"
            ]
        }
    }
}
